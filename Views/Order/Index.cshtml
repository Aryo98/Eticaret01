@model IEnumerable<Eticaret01.Models.AdminOrderModel>
@{
    ViewBag.Title = "Index";
}
<div class="container">
    <h2>Siparişler</h2>
    <hr />
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Order Number</th>
                <th>Order Date</th>
                <th>Order Status</th>
                <th>Total</th>
                <th>Product Count</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.OrderNumber</td>
                    <td>@item.OrderDate</td>
                    <td>@item.OrderState</td>
                    <td>@item.Total</td>
                    <td>@item.Count</td>
                </tr>
            }
        </tbody>
    </table>
</div>
